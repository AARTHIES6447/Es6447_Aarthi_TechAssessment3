package com.Aarthi.TechAssessment3;

import org.openqa.selenium.support.PageFactory;

public class RegisterActions {
	RegisterLocators RegisterLocators;
	String strFirstName, strLastName;
	public RegisterActions() {  
		this.RegisterLocators = new RegisterLocators();
		
		PageFactory.initElements(HelperClass.getDriver(), RegisterLocators);
	}
//	public void clickButton() {
//	 ((Object) RegisterLocators).click();
//    }
	public void setFirstName(String strFirstName) {
		RegisterLocators.FirstName.sendKeys(strFirstName);
	}
	public void setlasstName(String strLastName) {
		RegisterLocators.LastName.sendKeys(strLastName);
	}
	public void setEmail(String strEmail) {
		RegisterLocators.Email.sendKeys(strEmail);
	}
	public void setPassword(String strPassword) {
		RegisterLocators.Password.sendKeys(strPassword);
	}
	public void setConfirmPassword(String strConfirmPassword) {
		RegisterLocators.ConfirmPassword.sendKeys(strConfirmPassword);
	}
	public void getRegisterButton() {
		((Object) RegisterLocators.FirstName).Click();
	}
	
}
