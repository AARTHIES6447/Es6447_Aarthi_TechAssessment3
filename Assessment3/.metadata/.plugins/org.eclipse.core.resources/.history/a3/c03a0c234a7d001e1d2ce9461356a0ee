package com.Aarthi.TechAssessment3;

import org.openqa.selenium.support.PageFactory;

public class LoginActions {
LoginLocators objloginLoc;
	
	public void LoginAction() {
		objloginLoc = new LoginLocators();
		PageFactory.initElements(HelperClass.getDriver(), objloginLoc);
	}
	public void setUsername(String Email) {
		objloginLoc.Email.sendKeys(Email);
			
	}
	
	public void setPassword(String Password) {
		objloginLoc.Password.sendKeys(Password);
	}
	
	public void clickLoginBtn() {
		objloginLoc.Loginclick.click();
	}
	
	public void loginDetails(String Email,String Password) {
		setUsername(Email);
		setPassword(Password);
	}
 

}
